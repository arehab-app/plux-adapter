{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Launch Server",
            "type": "clr",
            "request": "launch",
            "preLaunchTask": "build server",
            "program": "${workspaceFolder}/PluxAdapter/Servers/bin/Debug/net451/PluxAdapter.exe",
            "args": [],
            "cwd": "${workspaceFolder}/PluxAdapter/Servers/bin/Debug/net451",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Attach Server",
            "type": "clr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
